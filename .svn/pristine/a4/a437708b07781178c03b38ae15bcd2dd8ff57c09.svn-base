<?xml version='1.0' encoding='UTF-8' ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:h="http://xmlns.jcp.org/jsf/html"
      xmlns:p="http://primefaces.org/ui">
    <h:head>
        <title>event details</title>
    </h:head>
    <h:body>
    <h:form>
    <p:panel header="Event details">
    <h:outputLabel for="from" value="Title:" />
    <p:outputLabel value="#{scheduleView.calendarEvent.title}"/><br/><br/>
    <h:outputLabel for="description" value="Description:" />
    <p:outputLabel value="#{scheduleView.calendarEvent.description}"/><br/><br/>
    <h:outputLabel for="location" value="Location:" />
    <p:outputLabel value="#{scheduleView.calendarEvent.location}"/><br/><br/>
    <h:outputLabel for="startDate" value="Start date:" />
    <p:outputLabel value="#{scheduleView.calendarEvent.startDate}"/><br/><br/>
    <h:outputLabel for="endDate" value="End date:" />
    <p:outputLabel value="#{scheduleView.calendarEvent.endDate}"/><br/><br/>
    <h:outputLabel for="indoorOutdoor" value="Indoor/Outdoor:" />
    <p:outputLabel value="#{scheduleView.calendarEvent.indoorOutdoor}"/><br/><br/>
    <p:commandButton value="See guests list" icon="ui-icon-person"/>
    <p:commandButton value="Return to Main Page" icon="ui-icon-home" action="mainPage"/>
    
    
   
    </p:panel>
    </h:form>   	
    </h:body>
</html>